  <style>
    body {
    font-family: Arial, sans-serif;
    background-color: #f5f5f5;
    margin: 0;
    padding: 0;
}

.container {
    max-width: 800px;
    margin: 20px auto;
    padding: 20px;
    background-color: #fff;
    border-radius: 5px;
    box-shadow: 0 0 10px rgba(0, 0, 0, 0.2);
}

.pdf-list {
    list-style: none;
    padding: 0;
}

.pdf-item {
    margin: 20px 0;
}

.pdf-category {
    font-size: 1.2em;
    color: #3498db;
    margin: 0;
}

.pdf-link {
    text-decoration: none;
    color: #333;
    display: flex;
    align-items: center;
    transition: color 0.3s ease-in-out;
}

.pdf-link:hover {
    color: #e74c3c;
}

.pdf-icon {
    font-size: 24px;
    margin-right: 10px;
}

@media (max-width: 768px) {
    .container {
        padding: 10px;
    }
}
.pdf-link.pdf-link1 {
    background-image: url('img1.jpg');
    background-size: cover;
    background-position: center;
    background-color: #3498db;
}


